{
  "name": "dictionary",
  "title": "dictionary",
  "version": "0.0.1",
  "description": "I18n dictionary for multi frameworks use",
  "directories": {
    "test": "test"
  },
  "bin": {
    "babel": "./node_modules/babel/bin/babel/index.js",
    "mocha": "./node_modules/mocha/bin/mocha"
  },
  "main": "./index.js",
  "scripts": {
